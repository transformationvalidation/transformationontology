  "http://purl.org/dc/elements/1.1/description" : [ {
    "@language" : "en",
    "@value" : "This is the ontology that supports transformation and validation processes of datasets available in data catalogues. The instances of the ontology are declarative descriptions used by a workflow engine. The workflow engine orchestrates tasks based on these declarative descriptions, aiming at transforming input datasets into the desired representation. The ontology extends existing W3C Web Recommendations such as DCAT and PROV.![image](ontology.png)"
  } ],
  "http://purl.org/dc/terms/license" : [ {
    "@id" : "https://creativecommons.org/licenses/by/4.0/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "This is the ontology that supports transformation and validation processes of datasets available in data catalogues. The instances of the ontology are declarative descriptions used by a workflow engine. The workflow engine orchestrates tasks based on these declarative descriptions, aiming at transforming input datasets into the desired representation. The ontology extends existing W3C Web Recommendations such as DCAT and PROV.![image](ontology.png)"
  } ],
